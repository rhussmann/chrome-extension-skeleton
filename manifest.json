{
  "manifest_version": 2,

  "name": "Chrome extension skeleton.",
  "description": "This extension helps get you started with Angular and Chrome extensions.",
  "version": "0.0.1",

  "icons": {
    "128": "block_128.png"
  },
  "browser_action": {
    "default_icon": "block_128.png",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
